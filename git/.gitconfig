[core]
    excludesfile = ~/.gitignore
[init]
    defaultBranch = main
[alias]
    a = add
    b = branch
    bc = checkout -b
    bs = show-branch
    s = status
    st = status -sb
    p = push
    pf = push --force
    pl = pull
    cm = commit -m
    pom = push origin main
    l = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    ll = log --graph --color --pretty=format:"%C(yellow)%H%C(green)%d%C(reset)%n%x20%cd%n%x20%cn%C(blue)%x20(%ce)%x20%C(cyan)[gpg:%GK%x20%G?]%C(reset)%n%x20%s%n"
    t = tag
[color]
	  ui = auto
